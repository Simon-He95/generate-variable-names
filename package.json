{
  "publisher": "simonhe",
  "name": "generate-variable-names",
  "displayName": "Generate Variable Names",
  "version": "0.0.9",
  "packageManager": "pnpm@8.15.8",
  "description": "generate-variable-names",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/generate-variable-names#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/generate-variable-names"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/generate-variable-names/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "assets/kv.png",
  "files": [
    "dist"
  ],
  "envFile": "${workspaceFolder}/.env",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "generate-variable-names.generateName",
          "group": "6_px"
        }
      ]
    },
    "commands": [
      {
        "command": "generate-variable-names.generateName",
        "title": "generate variable names"
      }
    ],
    "keybindings": [
      {
        "command": "generate-variable-names.generateName",
        "key": "cmd+shift+r",
        "when": "isMac"
      },
      {
        "command": "generate-variable-names.generateName",
        "key": "ctrl+shift+r",
        "when": "!isMac"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache --fix",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@simon_he/translate": "^0.0.5",
    "@types/node": "^18.19.39",
    "@types/vscode": "1.77.0",
    "@vscode-use/utils": "^0.1.6",
    "axios": "^1.7.2",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "esno": "^0.16.3",
    "pnpm": "^8.15.8",
    "rimraf": "^4.4.1",
    "translate-api": "^0.3.18",
    "tsup": "^6.7.0",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
}
