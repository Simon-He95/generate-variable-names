{
  "publisher": "simonhe",
  "name": "generate-variable-names",
  "displayName": "Generate Variable Names",
  "version": "0.0.14",
  "packageManager": "pnpm@8.15.9",
  "description": "generate-variable-names",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/generate-variable-names#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/generate-variable-names"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/generate-variable-names/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "assets/kv.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onCommand:generate-variable-names.generateName"
  ],
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "generate-variable-names.generateName",
          "group": "6_px"
        }
      ]
    },
    "commands": [
      {
        "command": "generate-variable-names.generateName",
        "title": "generate variable names"
      }
    ],
    "keybindings": [
      {
        "command": "generate-variable-names.generateName",
        "key": "cmd+shift+r",
        "when": "isMac"
      },
      {
        "command": "generate-variable-names.generateName",
        "key": "ctrl+shift+r",
        "when": "!isMac"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsdown src/index.ts --external vscode --format cjs",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@simon_he/translate": "^0.0.13",
    "@types/node": "^18.19.130",
    "@types/vscode": "1.77.0",
    "@vscode-use/utils": "^0.1.61",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^9.11.1",
    "eslint": "^9.39.2",
    "tsdown": "^0.18.1",
    "vitest": "^4.0.16"
  }
}
